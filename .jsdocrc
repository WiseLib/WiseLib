{
  "tags": {
      "allowUnknownTags": true
  },
  "source": {
      "exclude": [ "node_modules/" ]
  },
  "plugins": [ "plugins/markdown" ],
  "templates": {
    "systemName"            : "WiseLib",
    "footer"                : "Vrije Universiteit Brussel",
    "copyright"             :  "Â© 2015 WiseLib",
    "navType"               : "vertical",
    "theme"                 : "readable",
    "linenums"              : true,
    "analytics"             : { "ua": "UA-56218377-1", "domain": "wilma.vub.ac.be" },
    "collapseSymbols"       : false,
    "inverseNav"            : false,
    "outputSourceFiles"     : false,
    "outputSourcePath"      : true,
    "highlightTutorialCode" : true,
    "syntaxTheme"           : "dark"
  },
  "opts": {
    "destination": "./build/doc",
    "recurse": true,
    "tutorials": "./doc/tutorials"
  }
}
